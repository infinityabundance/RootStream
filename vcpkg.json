{
  "name": "rootstream",
  "version": "1.0.0",
  "description": "Secure peer-to-peer game streaming",
  "homepage": "https://github.com/infinityabundance/RootStream",
  "license": "MIT",
  "dependencies": [
    "sdl2",
    "libsodium",
    "opus",
    "cjson"
  ],
  "features": {
    "ffmpeg": {
      "description": "Use FFmpeg for video decoding",
      "dependencies": [
        {
          "name": "ffmpeg",
          "features": ["avcodec", "avformat", "avutil"]
        }
      ]
    },
    "web-dashboard": {
      "description": "Web dashboard for remote monitoring and management",
      "dependencies": [
        "libmicrohttpd",
        "libwebsockets",
        "cjson"
      ]
    }
  }
}
